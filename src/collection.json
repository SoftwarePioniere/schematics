{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "schematics": {
      "description": "A blank schematic.",
      "factory": "./schematics/index#schematics"
    },
    "page": {
      "aliases": [ "sopipage" ],
      "factory": "./page",
      "description": "Generate a angular page",
      "schema": "./page/schema.json"
    },
    "my-other-schematic": {
      "description": "A schematic that uses another schematics.",
      "factory": "./my-other-schematic"
    },
    "my-schematic": {
      "description": "An example schematic",
      "factory": "./my-schematic/index#mySchematic",
      "schema": "./my-schematic/schema.json"
    },
    "modal-dialog": {
      "aliases": [ "modaldialog" ],
      "factory": "./modal-dialog",
      "description": "Generate a modal dialog",
      "schema": "./modal-dialog/schema.json"
    },
    "modal-auswahl": {
      "aliases": [ "modalauswahl" ],
      "factory": "./modal-auswahl",
      "description": "Generate a delete modal",
      "schema": "./modal-auswahl/schema.json"
    },
    "swagger-ngrx": {
      "aliases": [ "swagger" ],
      "factory": "./swagger-ngrx",
      "description": "Generate actions and effects for swagger client",
      "schema": "./swagger-ngrx/schema.json"
    },
    "generate-index": {
      "aliases": [ "generateindex" ],
      "factory": "./generate-index",
      "description": "Generate index.ts file in targetpath with all files from sourcepath",
      "schema": "./generate-index/schema.json"
    }
  }
}
