{
  "$schema": "http://json-schema.org/schema",
  "id": "swaggerngrx",
  "title": "Generate actions and effects for swagger client",
  "type": "object",
  "properties": {
    "index": {
      "type": "number",
      "default": 1
    },
    "clientname": {
      "type": "string"
    },
    "method": {
      "type": "string"
    },
    "service": {
      "type": "string"
    },
    "requestparams": {
      "type": "string"
    },
    "responseparams": {
      "type": "string"
    },
    "targetpath": {
      "type": "string"
    },
    "group": {
      "type": "string"
    },
    "actiontype": {
      "type": "string"
    },
    "useNgrxManager": {
      "type": "string"
    }
  },
  "required": [
    "clientname",
    "method",
    "service",
    "requestparams",
    "responseparams",
    "targetpath",
    "actiontype"
  ]
}
