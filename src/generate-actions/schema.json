{
  "$schema": "http://json-schema.org/schema",
  "id": "generateactions",
  "title": "Generate actions for all json files with 'postfix'",
  "type": "object",
  "properties": {
    "index": {
      "type": "number",
      "default": 1
    },
    "sourcepath": {
      "type": "string"
    },
    "postfix": {
      "type": "string"
    }
  },
  "required": [
    "sourcepath",
    "postfix"
  ]
}
